{
  "name": "react-handlebars-typescript-sass-lint",
  "version": "1.0.0",
  "description": "A sample project that demonstrates how to use React with Handlebars, TypeScript, and SCSS.",
  "main": "index.js",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "react",
    "handlebars",
    "typescript",
    "scss",
    "lint"
  ],
  "author": "aki-mia",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "handlebars": "^4.7.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@npmcli/fs": "^1.0.0",
    "@testing-library/dom": "^8.11.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-react": "^1.1.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.4.1",
    "sass": "^1.41.1",
    "ts-jest": "^27.1.1",
    "typescript": "^4.6.2",
    "vite": "^2.7.9"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
